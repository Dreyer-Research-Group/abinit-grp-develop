  tmp_s_bigdft=`echo "${afb_bigdft_ok}" | sed -e 's/^yes/ok/;s/^no/broken/' | awk '{printf("%-8s",$1);}'`
  tmp_i_bigdft=`echo "${enable_bigdft}" | awk '{printf("%-8s",$1);}'`
  tmp_c_bigdft=`echo "${afb_bigdft_custom}" | awk '{printf("%-8s",$1);}'`
  tmp_t_bigdft=`echo "${afb_bigdft_tricks}" | awk '{printf("%-8s",$1);}'`
  tmp_s_libpsml=`echo "${afb_libpsml_ok}" | sed -e 's/^yes/ok/;s/^no/broken/' | awk '{printf("%-8s",$1);}'`
  tmp_i_libpsml=`echo "${enable_libpsml}" | awk '{printf("%-8s",$1);}'`
  tmp_c_libpsml=`echo "${afb_libpsml_custom}" | awk '{printf("%-8s",$1);}'`
  tmp_t_libpsml=`echo "${afb_libpsml_tricks}" | awk '{printf("%-8s",$1);}'`
