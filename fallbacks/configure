  tmp_i_linalg=`echo "${enable_linalg}" | awk '{printf("%-8s",$1);}'`
  tmp_c_linalg=`echo "${afb_linalg_custom}" | awk '{printf("%-8s",$1);}'`
  tmp_t_linalg=`echo "${afb_linalg_tricks}" | awk '{printf("%-8s",$1);}'`
  tmp_s_netcdf4=`echo "${afb_netcdf4_ok}" | sed -e 's/^yes/ok/;s/^no/broken/' | awk '{printf("%-8s",$1);}'`
  tmp_i_netcdf4=`echo "${enable_netcdf4}" | awk '{printf("%-8s",$1);}'`
